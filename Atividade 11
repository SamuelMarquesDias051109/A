programa {
  funcao inicio() {
  inteiro num, contador, divisores
  divisores=0
  escreva ("Digite um numero: \n")
  leia(num)

  para (contador = 1; contador <=num; contador++){
  se (num % contador == 0) {
    divisores = divisores + 1
  }
  }
  se ( divisores == 2) {
    escreva( " O numero ", num, " é Primo. \n")
  } senao {
    escreva("O numero ", num, " não é primo. \n")
  }

  }
}
