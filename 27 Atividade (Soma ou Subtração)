numero1 = input("Digite um número: ")
numero2 = int(input("Digite outro número: "))
num1 = numero1
num2 = numero2

if num1 > num2 :
    calculo = num1 - num2
    print ("A subtração desses números é: ", calculo)
    
else :
    calculo1 = num1 + num2
    print ("A soma desses números é: ", calculo1)
