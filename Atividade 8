programa {
  funcao inicio() {
    inteiro numero, resultado, contador
    escreva ("Digite um numero: \n")
    leia (numero)

    para(contador = 1; contador <=10;contador++) {
    resultado = numero*contador
    escreva(numero, " x ", contador, " = ", resultado, "\n") }
  }
}

Outra maneira

programa {
  funcao inicio() {
    inteiro numero, resultado, contador
    escreva ("Digite um numero: \n")
   leia(numero) 
   contador= 1
   enquanto (contador <=10) {
    resultado= numero*contador
    escreva(numero, " x ", contador, " = ", resultado, "\n")
    contador= contador+1 }
  }
}
