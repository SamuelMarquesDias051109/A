nome = input("Digite seu nome: ")
idade = float(input("Digite sua idade: "))
print ("\n")
print ("Nome: ", nome)   
if idade >= 18  and idade < 70 :
    print("Voto Obrigatorio")
    
elif idade >= 16 and idade <= 17 : 
    print ("Voto facultativo.")

elif idade >= 70 : 
    print ("Voto facultativo.")
    
else :
    print("NÃ£o precisa votar.")
